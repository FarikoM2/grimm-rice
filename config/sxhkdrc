# Lanzar terminal
super + Return
    urxvt

# Lanzar rofi (launcher)
super + d
    rofi -show drun

# Cerrar ventana
super + q
    bspc node -c

# Mover foco con flechas
super + {Left,Down,Up,Right}
    bspc node -f {west,south,north,east}

# Mover ventana con flechas
super + shift + {Left,Down,Up,Right}
    bspc node -s {west,south,north,east}
